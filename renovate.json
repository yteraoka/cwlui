{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": ["yteraoka"],
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["boto3", "botocore"],
      "groupName": "boto"
    },
    {
      "matchPackagePatterns": ["flask", "Werkzeug"],
      "groupName": "flask"
    }
  ]
}
